# mount point	fstype    device    	[device2]

/misc       mtd         /dev/block/mtdblock3
/boot       mtd         /dev/block/mtdblock4
/recovery   mtd         /dev/block/mtdblock5
/system     yaffs2      /dev/block/mtdblock7
/cache      yaffs2      /dev/block/mtdblock8
/data       yaffs2      /dev/block/mtdblock9
/sdcard     vfat        /dev/block/mmcblk0p1        /dev/block/mmcblk0
/sd-ext     ext3        /dev/block/mmcblk0p2

